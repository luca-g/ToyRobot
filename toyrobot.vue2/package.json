{
  "name": "toyrobot.vue2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "api": "yarn del && SET JAVA_OPTS=-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true && openapi-generator-cli generate -i https://localhost/toyrobotapi/swagger/v1/swagger.json --generator-name typescript-axios -o gen/toyRobotApi --config toyRobotApi.json && cd gen/toyRobotApi && yarn install && yarn build && rd /s /q node_modules && cd ..\\.. && yarn add file:gen/toyRobotApi",
    "del": "if exist \"gen\\toyRobotApi\" (rmdir \"gen\\toyRobotApi\" /q /s)",
    "watch": "vue-cli-service build --watch"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@vue/composition-api": "^1.5.0",
    "core-js": "^3.8.3",
    "toy-robot-axios": "file:gen/toyRobotApi",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-cookies": "^1.8.1",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vuetify": "^2.6.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "swagger-codegen-openapi": "^1.0.7",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vue-cli-plugin-vuetify": "^2.4.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  }
}
